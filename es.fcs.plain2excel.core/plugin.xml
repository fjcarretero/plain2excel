<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="es.fcs.plain2excel.ui.View2"
            id="es.fcs.plain2excel.ui.view"
            name="name">
           
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               id="es.fcs.plain2excel.ui.view"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="false"
               standalone="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="es.fcs.plain2excel.ui.PreferredPerspectivePartListener">
      </startup>
   </extension>
</plugin>
